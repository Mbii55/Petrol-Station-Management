<body class="dark-theme">
  <div class="bg-light min-vh-100 d-flex flex-row align-items-center dark:bg-transparent">
    <div class="container">
      <div class="row justify-content-center">
        <div class="col-md-6">
          <div class="clearfix">
            <h1 class="float-start display-3 me-4">404</h1>
            <h4 class="pt-3">Oops! You're lost.</h4>
            <p class="text-medium-emphasis">The page you are looking for was not found.</p>
          </div>
          <div class="input-group"><span class="input-group-text">
              <svg class="icon">
                <use xlink:href="/static/vendors/@coreui/icons/svg/free.svg#cil-magnifying-glass"></use>
              </svg></span>
            <form action="/found" id="found" method="post">
              <input class="form-control" id="prependedInput" size="14.5" type="text"
                placeholder="What are you looking for?" name="page">
            </form>
            <button class="btn btn-info" type="submit" form="found" style="margin-left: 252px;">Search</button>
          </div>
        </div>
      </div>
    </div>
  </div>
